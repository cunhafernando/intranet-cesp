        <td class="d-none d-xl-table-cell"><%= link_to 'Apagar', amount_of_upa_service, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
