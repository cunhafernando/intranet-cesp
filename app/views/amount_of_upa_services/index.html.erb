        <td class="d-none d-xl-table-cell"><%= amount_of_upa_service.yellow_time %></td>
        <td class="d-none d-xl-table-cell"><%= amount_of_upa_service.yellow_in_the_goal %></td>
        <td class="d-none d-xl-table-cell"><%= amount_of_upa_service.yellow_off_target %></td>
        <td class="d-none d-xl-table-cell"><%= amount_of_upa_service.red_in_the_goal %></td>
        <td class="d-none d-xl-table-cell"><%= amount_of_upa_service.red_off_target %></td>
