      <th class="d-none d-xl-table-cell">Amarelo fora da meta</th>
      <th class="d-none d-xl-table-cell">Vermelho na meta</th>
      <th class="d-none d-xl-table-cell">Vermelho fora da meta</th>
      <th class="d-none d-xl-table-cell" colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  <% @amount_of_upa_services.each do |amount_of_upa_service| %>
      <tr>
        <td class="d-none d-xl-table-cell"><%= amount_of_upa_service.year_month %></td>
        <td class="d-none d-xl-table-cell"><%= amount_of_upa_service.registered %></td>
        <td class="d-none d-xl-table-cell"><%= amount_of_upa_service.classified %></td>
        <td class="d-none d-xl-table-cell"><%= amount_of_upa_service.attended %></td>
        <td class="d-none d-xl-table-cell"><%= amount_of_upa_service.welcomed %></td>
        <td class="d-none d-xl-table-cell"><%= amount_of_upa_service.service_time %></td>
        <td class="d-none d-xl-table-cell"><%= amount_of_upa_service.green_time %></td>
